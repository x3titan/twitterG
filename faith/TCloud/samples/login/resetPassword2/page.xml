    alert("新密码的长度至少需要6位，请重新输入");
    return;
}
if (password1 != password2) {
    alert("对不起，您两次输入的密码不一致，请重新输入。");
    return;
} 
eio.appendString8(password1);

]]></script1>
      <script2>if (session.get("mobileCheck") != "true") {
