    <taskSetItem name="reg">
      <script1><![CDATA[g5.mobile = ee.getEditText(ee.getControl("edit1"));
g5.idName = ee.getEditText(ee.getControl("edit2"));
g5.pwd1 = ee.getEditText(ee.getControl("edit5"));
g5.pwd2 = ee.getEditText(ee.getControl("edit6"));

if(g5.pwd1.length<=0){
    alert("密码不能为空");
    return; 
}
if(g5.pwd1 != g5.pwd2){
    alert("两次输入的密码不一致");
    return; 
}
if (g5.mobile.length!=11) {
    alert("输入的手机号无效");
    return;
}
if (g5.idName.length<2) {
    alert("真实姓名无效");
    return;
}
eio.appendString8(g5.mobile);
eio.appendString8(g5.idName);
eio.appendString8(g5.pwd1);
eio.appendInt32(g5.tid);
eio.appendInt32(g5.uid);

]]></script1>
      <script2><![CDATA[var mobile = eio.readString8();
var idName= eio.readString8();
var password = eio.readString8();
var teamId = eio.readInt32();
var userId = eio.readInt32();
var passwordMd5 = es.getMd5(password);
var sql,sqlResult;  

sql="select * from userInfo where (mobile='" + db.encode(mobile) +"' or idName='" + db.encode(idName) + "') and enabled='T'";                   
sqlResult = db.executeQuery("PJ", sql);  
if (db.read(sqlResult)) {
    eio.appendString8("F");
    eio.appendString8("手机号或者姓名已经被注册过了");
    return;
}

sql = "exec createUser '"+db.encode(mobile)+"','"+db.encode(idName)+"','"+db.encode(passwordMd5)+"';";
sqlResult = db.executeQuery("PJ", sql);   
if (sqlResult == null) {
    eio.appendString8("F");
    eio.appendString8("系统繁忙请稍后再试");
    return;
}
db.read(sqlResult);
if (db.getInt32(sqlResult, "errorCode")!=0) {    
    eio.appendString8("F");
    eio.appendString8(db.getString(sqlResult, "errorString"));
    return;
}   

//自动加入项目组
if ((teamId>0)&amp;&amp;(userId>0)) {
    sql = "exec teamInvate " + userId + ",'" + db.encode(mobile) + "'," + teamId;
    sqlResult = db.executeQuery("PJ", sql);   
    if (sqlResult == null) {
        eio.appendString8("F");
        eio.appendString8("系统繁忙请稍后再试");
        return;
    }
    db.read(sqlResult);
    if (db.getInt32(sqlResult, "errorCode")<0) {    
        eio.appendString8("F");
        eio.appendString8(db.getString(sqlResult, "errorString"));
        return;
    }   
}

eio.appendString8("T");
es.logCommon("ProjectAdmin:用户注册成功,mobile=" + mobile + ",idName=" + idName);
]]></script2>
      <script3>var resultCode=eio.readString8();     
if (resultCode=="F") {  
    alert("无法注册新用户：" + eio.readString8());
    return;
}  

alert("注册用户成功，您可以使用刚才注册的手机或者真实姓名登录");
window.location.href = ee.getUrl("/login");


</script3>
